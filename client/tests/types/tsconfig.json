{
	"compilerOptions": {
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"target": "es5",
		"module": "es6",
		"sourceMap": true,
		"esModuleInterop": true,
		"lib": ["es2015", "es2016", "es2017", "dom"]
	}
}