{
  "name": "seniority-test-client",
  "version": "0.0.0",
  "description": "Seniority test client",
  "main": "js/index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf js && rm -rf css && webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dggluz/seniority-test.git"
  },
  "author": "dggluz@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dggluz/seniority-test/issues"
  },
  "homepage": "https://github.com/dggluz/seniority-test#readme",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/jquery": "^3.3.9",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.0",
    "ts-jest": "^23.10.1",
    "ts-loader": "^5.1.1",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {}
}
